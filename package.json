    "type": "git",
    "url": "git+https://github.com/kohded/next-starter.git"
  },
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "format": "prettier '**/*.{css,html,js,jsx,json,less,md,scss,ts,tsx,yml}' -c -w",
    "lint": "npm run lint:eslint && npm run lint:stylelint",
    "lint:eslint": "eslint '**/*.{js,jsx,ts,tsx}'",
    "lint:stylelint": "stylelint **/*.{css,less,scss}",
    "prebuild": "npm run format && npm run lint",
    "setup": "npm run setup:husky && npm run setup:ssl",
    "setup:husky": "husky install && husky add .husky/pre-commit \"npx --no-install lint-staged\"",
    "setup:ssl": "cross-env mkdir -p config/ssl && mkcert -cert-file config/ssl/cert.pem -key-file config/ssl/key.pem localhost",
    "start": "next start"
  },
  "version": "0.1.0"
